@model IEnumerable<NEC.Models.admin>

@{
    ViewBag.Title = "Index";
}
<head>
    <meta name="viewport" content="width=device-width" />
    <link href="@Url.Content("~/Content/StyleSheet1.css")" rel="stylesheet" />
</head>


<div class="jai">
    <centre><img src="data:image/jpg;base64," width="150" height="90" alt="NEC"></centre>
</div>
<div class="jai1">
    <h1>National Engineering College</h1>

</div>
<div class="jai2">
    <p1>
        Counselling Code 4962 <br />
        NIRF 2022 Rank 169
    </p1>
</div>

<h2>Index</h2>


<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            ntre
            @Html.DisplayNameFor(model => model.company_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.@event)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.C10th_criteria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.C12th_criteria)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CPGA)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.website)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.company_name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.@event)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.C10th_criteria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.C12th_criteria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CPGA)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.website)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.id }) |

                @Html.ActionLink("Delete", "Delete", new { id = item.id })
            </td>
        </tr>
    }

</table>
 